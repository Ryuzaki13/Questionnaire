<Window x:Class="Questionnaire.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Questionnaire"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Frame NavigationUIVisibility="Hidden" Name="AppFrame"></Frame>

        <Border Name="Message" VerticalAlignment="Bottom" Padding="10" Margin="10" Visibility="Collapsed">
            <TextBlock Foreground="White" FontWeight="Bold" VerticalAlignment="Center" HorizontalAlignment="Center" Name="MessageText"></TextBlock>
        </Border>
    </Grid>
</Window>
